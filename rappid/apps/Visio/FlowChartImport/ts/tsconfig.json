{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "lib": [
      "dom"
    ],
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "skipLibCheck": true
  },
  "compileOnSave": false
}
